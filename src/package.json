{
  "name": "@getany/core",
  "version": "2.0.0",
  "description": "Clean architecture implementation of getany media downloader",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "getany": "dist/presentation/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node presentation/cli/index.ts",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "media",
    "downloader",
    "instagram",
    "youtube",
    "twitter",
    "clean-architecture",
    "typescript"
  ],
  "author": "getany",
  "license": "MIT",
  "dependencies": {
    "glob": "^10.3.10",
    "node-fetch": "^2.7.0",
    "puppeteer": "^21.6.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.9",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/*.test.ts",
      "**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/dist/**"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}